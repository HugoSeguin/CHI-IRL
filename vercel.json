{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "250mb",
                "runtime": "python3.9",
                "requirementsPath": "requirements-slim.txt",
                "installCommand": "pip install -r requirements-slim.txt --no-cache-dir --no-deps && pip install google-api-python-client google-auth-oauthlib --no-cache-dir"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ],
    "env": {
        "PYTHON_VERSION": "3.9"
    }
}
